{
  "name": "bjohansebas",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "fetch:sponsors": "node ./scripts/fetch-sponsors.mjs",
    "fetch:sponsors:env": "node --env-file=.env ./scripts/fetch-sponsors.mjs",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "check": "oxlint . && prettier --check .",
    "check:fix": "oxlint --fix . && prettier --write .",
    "lint:staged": "lint-staged",
    "astro": "astro",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/sitemap": "3.7.0",
    "@astrojs/tailwind": "6.0.2",
    "@astrojs/vercel": "9.0.4",
    "@fontsource/geist-sans": "5.2.5",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "5.17.1",
    "date-fns": "^4.1.0",
    "hotkeypad": "1.0.2",
    "satori": "^0.19.2",
    "sharp": "^0.34.5",
    "tailwindcss": "3.4.19",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "husky": "9.1.7",
    "lightningcss": "1.31.1",
    "lint-staged": "16.2.7",
    "lucide-astro": "0.556.0",
    "oxlint": "1.42.0",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-tailwindcss": "0.7.2"
  },
  "packageManager": "pnpm@10.15.0",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=20.5.0",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "10.15.0",
      "onFail": "error"
    }
  }
}
